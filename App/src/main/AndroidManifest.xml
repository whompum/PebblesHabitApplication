<manifest package="com.whompum.app"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:name="com.whompum.app.MainApplication">
        <activity android:name="com.whompum.app.LaunchActivity"
                  android:theme="@style/AppThemeMain.LaunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>