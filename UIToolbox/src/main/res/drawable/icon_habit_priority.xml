<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--
        State Priority-High completed/uncompleted
    -->
    <item android:drawable="@drawable/icon_star_empty" app:state_priority_high_uncompleted="true" />
    <item android:drawable="@drawable/icon_star_full" app:state_priority_high_completed="true" />

    <!--
        State Priority-Normal completed/uncompleted
    -->
    <item android:drawable="@drawable/icon_star_empty" app:state_priority_normal_uncompleted="true" />
    <item android:drawable="@drawable/icon_star_full" app:state_priority_normal_completed="true" />

</selector>